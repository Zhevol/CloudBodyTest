<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <size android:width="@dimen/dp_064" android:height="@dimen/dp_032" />
            <stroke android:width="@dimen/dp_001" android:color="@color/colorWhite80" />
            <corners android:radius="@dimen/dp_016" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <size android:width="@dimen/dp_064" android:height="@dimen/dp_032" />
            <stroke android:width="@dimen/dp_001" android:color="@color/colorWhitePure" />
            <corners android:radius="@dimen/dp_016" />
        </shape>
    </item>
    <item android:state_focused="false">
        <shape>
            <size android:width="@dimen/dp_064" android:height="@dimen/dp_032" />
            <stroke android:width="@dimen/dp_001" android:color="@color/colorWhitePure" />
            <corners android:radius="@dimen/dp_016" />
        </shape>
    </item>
</selector>